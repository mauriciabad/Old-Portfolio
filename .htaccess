<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{HTTP_HOST} ^www\.mauriciabad\.com [NC]
RewriteRule ^(.*)$ https://mauriciabad.com/$1 [L,R=301]
</IfModule>
<Files ~ "^\.(htaccess|htpasswd)$">
deny from all
</Files>
ErrorDocument 404 /404.html
RedirectMatch 301 ^/CV/(.*)$ https://www.mauriciabad.com/Maurici_Abad_Gutierrez_-_Curriculum_Vitae.pdf
order deny,allow
