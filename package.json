{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Portfolio online Maurici Abad",
  "keywords": [
    "vue",
    "vuepress",
    "portfolio",
    "mauriciabad",
    "blog"
  ],
  "homepage": "https://github.com/mauriciabad/Portfolio#readme",
  "bugs": {
    "url": "https://github.com/mauriciabad/Portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mauriciabad/Portfolio.git"
  },
  "license": "MIT",
  "author": {
    "name": "Maurici Aabd Gutierrez",
    "url": "https://mauriciabad.com"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "styles",
    "layouts",
    "global-components",
    "components"
  ],
  "scripts": {
    "build": "vuepress build docs",
    "dev": "vuepress dev docs",
    "docs:build": "vuepress build docs --temp docs/.temp",
    "docs:dev": "vuepress dev docs --temp docs/.temp",
    "lint": "eslint . --ext .js,.vue",
    "prepublishOnly": "npm run test && npm run docs:build && conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "start": "yarn dev",
    "test": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@vuepress/plugin-blog": "^1.9.2",
    "@vuepress/plugin-medium-zoom": "1.5.4",
    "@vuepress/plugin-nprogress": "1.5.4",
    "@vuepress/plugin-pwa": "1.5.4",
    "@vuepress/plugin-search": "1.5.4",
    "bootstrap-4-stylus": "^1.0.7",
    "dayjs": "^1.8.36",
    "lodash": "^4.17.20",
    "remove-markdown": "^0.3.0",
    "vue-feather-icons": "^5.1.0",
    "vuepress-plugin-smooth-scroll": "^0.0.10"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-sherry": "0.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "sort-package-json": "^1.46.0",
    "vuepress": "^1.5.4"
  }
}
